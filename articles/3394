		<a data-ga-event="pc:share:weibo" href="http://service.weibo.com/share/share.php" class="share iconfont icon-weibo" ></a>
		<!-- 腾讯微博 -->
		<a data-ga-event="pc:share:tengxunweixin" href="http://share.v.t.qq.com/index.php" class="share iconfont icon-tengxunweibo" ></a>
		<!-- QQ空间 -->
		<a data-ga-event="pc:share:kongjian" href="http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey" class="share iconfont icon-kongjian" ></a>
		<!-- 豆瓣 -->
		<a data-ga-event="pc:share:douban" href="http://www.douban.com/share/service" class="share iconfont icon-douban" ></a>
		<!-- linkedIn -->
		<a data-ga-event="pc:share:linkedin" href="http://www.linkedin.com/shareArticle" class="share iconfont icon-linkedin" ></a>
		
			<a data-ga-event="pc:favor:aritcle" href="#" class="favor iconfont icon-heart"><span class="num ">40</span></a>
	</div>
</div>
		</div>
	</div>
</div>
			<!-- 传入参数：relateds, votes, type -->
<!-- type取值为short, long-article -->



<!-- 长文章 -->
<div class="com-related-articles">
	<div class="related-articles-bd">
		<div class="packery-container articles">
                    <div class="packery-item article size1x2">
                        <!-- 重点文章格子，1x2的占位，智能占位 -->
<!-- 传入参数：article -->
